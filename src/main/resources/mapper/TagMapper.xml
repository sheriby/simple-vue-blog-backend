<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ink.sher.vueblog.mapper.TagMapper">

    <select id="getTagIdByBlogId" resultType="java.lang.Integer">
        select tagid from t_blog_tags where blogid = #{id}
    </select>
</mapper>
